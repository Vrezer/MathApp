<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mathapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Cone"></activity>
        <activity android:name=".Sphere" />
        <activity android:name=".Cuboid" />
        <activity android:name=".Cube" />
        <activity android:name=".Circle" />
        <activity android:name=".TrianglesRectangular" />
        <activity android:name=".TriangleEquilateral" />
        <activity android:name=".Rectangle" />
        <activity android:name=".Square" />
        <activity android:name=".GeometricString" />
        <activity android:name=".ArithmeticString" />
        <activity android:name=".QuadraticFunction" />
        <activity android:name=".LinearFunction" />
        <activity android:name=".MultiplicationTable" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>